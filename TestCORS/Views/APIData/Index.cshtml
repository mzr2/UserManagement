            <th scope="col">
                Nama
            </th>
            <th scope="col">
                Action
            </th>
        </tr>
    </thead>
    <tbody id="listpokemon">
    </tbody>
</table>
<!-- Button trigger modal -->
<button type="button" class="btn btn-primary tes" data-toggle="modal" data-target="#exampleModal">
    Launch demo modal
</button>

